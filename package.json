{
  "name": "scatter-seed-server",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "license": "GPL-3.0 License",
  "scripts": {
    "build": "swc --delete-dir-on-start -d build src",
    "build:dev": "npm run build && cross-env NODE_ENV=development node --inspect=7121 build/index.js",
    "start": "npm run db:gen && npm run build && node build/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --ext ts --exec \"npm run build:dev\"",
    "db:gen": "npx prisma generate",
    "db:seed": "npx prisma db seed",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:status": "npx prisma migrate status"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/connect-redis": "^0.0.18",
    "@types/connect-sqlite3": "^0.9.1",
    "@types/cors": "^2.8.12",
    "@types/express-jwt": "^6.0.4",
    "@types/express-session": "^1.17.4",
    "@types/graphql": "^14.5.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "prisma": "^3.11.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.163",
    "apollo-server-core": "^3.6.3",
    "apollo-server-express": "^3.6.3",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.1",
    "connect-sqlite3": "^0.9.13",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "express-session": "^1.17.2",
